{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "ExpirationInMinutes": 20,
    "RefreshExpirationInDays": 7,
    "Issuer": "Learn@Home",
    "Audience": "account",
    "RefreshTokenCookieName": "refresh_token",
    "SessionTokenCookieName": "session_token"
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "*"
}
